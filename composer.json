{
    "name": "verbb/cp-nav",
    "description": "Control Panel Nav is a Craft CMS plugin to help manage your Control Panel navigation.",
    "type": "craft-plugin",
    "version": "2.0.9",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "cp nav",
        "control panel nav",
        "control panel navigation"
    ],
    "support": {
        "email": "support@verbb.io",
        "issues": "https://github.com/verbb/cp-nav/issues?state=open",
        "source": "https://github.com/verbb/cp-nav",
        "docs": "https://github.com/verbb/cp-nav",
        "rss": "https://github.com/verbb/cp-nav/commits/v2.atom"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Verbb",
            "homepage": "http://verbb.io"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "verbb\\cpnav\\": "src/"
        }
    },
    "extra": {
        "name": "Control Panel Nav",
        "handle": "cp-nav",
        "description": "Control Panel Nav helps you managing your Control Panel navigation.",
        "schemaVersion": "2.0.0",
        "hasCpSettings": true,
        "hasCpSection": false,
        "documentationUrl": "https://github.com/verbb/cp-nav",
        "changelogUrl": "https://raw.githubusercontent.com/verbb/cp-nav/craft-3/CHANGELOG.md",
        "class": "verbb\\cpnav\\CpNav"
    }
}
